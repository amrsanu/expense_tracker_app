{
  "functions": {
    "api": {
      "runtime": "@vercel/python",
      "memory": 512,
      "maxDuration": 10,
      "environment": {},
      "build": {
        "env": {
          "PIP_TARGET": "/vercel/path0/.vercel/requirements"
        }
      },
      "entrypoint": "app.server"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/:1" }
  ]
}
